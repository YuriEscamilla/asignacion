<?xml version="1.0"?>
<odoo>
    <!--CATEGORIA DE SEGURIDAD  -->
    <record id="module_asignacion_category" model="ir.module.category">
        <field name="name">ASIGNACION DE TRAMITES</field>
    </record>
    <!-- ASIGNACION DE TRAMITES-->
    <record id="asignacion_tramites_group_user" model="res.groups">
        <field name="name">Asignación de tramite GENERAL</field>
        <field name="category_id" ref="module_asignacion_category"/>
        <field name="implied_ids" eval="[(4,ref('base.group_user'))]"/>
    </record>

